SELECT login, COUNT("inDelivery")
FROM "Couriers" AS с
INNER JOIN "Orders" AS o ON с.id = o."courierId"
WHERE "inDelivery" = true
GROUP BY login;

SELECT track,
 CASE
  WHEN finished = true THEN 2
  WHEN canсelled = true THEN -1
  WHEN "inDelivery" = true THEN 1
  ELSE 0
 END AS status
FROM "Orders";